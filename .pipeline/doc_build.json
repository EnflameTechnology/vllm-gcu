{
    "units": [
        {
            "name": "pdf",
            "source": "docs",
            "command": "make host-html && make latexpdf -C content",
            "publish": [
                {
                    "from": "docs/content/build/latex/*.pdf",
                    "to": "pdf/"
                }
            ]
        },
        {
            "name": "html",
            "source": "docs/content/source",
            "command": "sphinx-build ./ _build",
            "publish": [
                {
                    "from": "docs/content/source/_build",
                    "to": "html/"
                }
            ]
        }
    ]
}