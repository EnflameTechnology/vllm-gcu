{
  "torch2.5_all":{
    "dependences" : [ "all_default", "torch_gcu250" ],
    "framework":{
        "torch":"2.5.0"
    }
  },
  "torch2.5.1_all":{
    "dependences" : [ "all_default", "torch_gcu251" ],
    "framework":{
        "torch":"2.5.1"
    }
  },
  "all":{
    "dependences" : [ "all_default", "torch_gcu230" ],
    "framework":{
        "torch":"2.3.0"
    }
  },
  "all_default" :
  {
    "deb" : ["topsaten", "eccl","topsfactor", "tops-sdk"],
    "dependences" : [ "runtime"],
    "whl" :
    {
      "3.10" : ["vllm-gcu==0.7.2","xformers","tops-extension"],
      "3.9" : ["vllm-gcu==0.7.2","xformers","tops-extension"]
    },
    "special_cmd" : [
      {
          "name" : "vllm",
          "install" : "sudo python -m pip install vllm==0.7.2",
          "uninstall" : "sudo python -m pip uninstall vllm -y || true"
      },
      {
          "name" : "numpy",
          "install" : "sudo python -m pip install numpy==1.22.4",
          "uninstall" : "sudo python -m pip uninstall numpy -y || true"
      },
      {
          "name" : "opencv-python",
          "install" : "sudo python3.10 -m pip install opencv-python==4.10.0.84",
          "uninstall" : "sudo python3.10 -m pip uninstall opencv-python -y || true"
      },
      {
          "name" : "opencv-python-headless",
          "install" : "sudo python3.10 -m pip install opencv-python-headless==4.10.0.84",
          "uninstall" : "sudo python3.10 -m pip uninstall opencv-python-headless -y || true"
      },
      {
          "name" : "protobuf",
          "install" : "sudo python3.10 -m pip install protobuf==3.20.1",
          "uninstall" : "sudo python3.10 -m pip uninstall protobuf -y || true"
      }
    ]
  },
  "torch_gcu230" :
  {
    "whl" :
    {
      "3.10" : ["torch-gcu==2.3.0"],
      "3.8" : ["torch-gcu==2.3.0"]
    }
  },
  "torch_gcu250" :
  {
    "whl" :
    {
      "3.10" : ["torch-gcu==2.5.0"],
      "3.9" : ["torch-gcu==2.5.0"]
    }
  },
  "torch_gcu251" :
  {
    "whl" :
    {
      "3.10" : ["torch-gcu==2.5.1"],
      "3.9" : ["torch-gcu==2.5.1"]
    }
  },
  "runtime" :
  {
    "deb" : [],
    "dependences" : [],
    "rpm" : [],
    "run" : [ "topsruntime","topscc" ],
    "whl" :
    {
      "3.10" : [],
      "3.6" : [],
      "3.8" : [],
      "3.9" : []
    }
  }
}
